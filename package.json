{
  "name": "@xornot/tipsy",
  "description": "Package updater",
  "version": "1.0.2",
  "author": "Chris Ackerman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xornot-io/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/xornot-io/tipsy/issues"
  },
  "homepage": "https://github.com/xornot-io/tipsy#readme",
  "bin": "lib/index.js",
  "files": [
    "css/",
    "dist/",
    "lib/",
    "scss/"
  ],
  "scripts": {
    "test": "tsc --noEmit && tslint \"src/**/*.ts?(x)\" && cross-env NODE_ENV=test jest --passWithNoTests",
    "start": "lib/index.js",
    "clean": "del-cli lib dist css coverage && jest --clearCache",
    "prepare": "npm run build",
    "build": "npm run build:js && npm run build:d.ts",
    "build:d.ts": "tsc --emitDeclarationOnly",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --extensions .ts,.tsx,.js,.jsx --copy-files"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/compare-versions": "^3.0.0",
    "@types/prompts": "^2.0.0",
    "@types/source-map-support": "^0.5.0",
    "@xornot/memo": "^1.3.3",
    "chalk": "^2.4.2",
    "compare-versions": "^3.4.0",
    "package-json": "^6.3.0",
    "prompts": "^2.1.0",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "del-cli": "^2.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  }
}
