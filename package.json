{
  "name": "newest",
  "description": "Interactively update dependency versions in package.json, to their newest release (non-tagged) versions.",
  "version": "1.0.5",
  "author": "Chris Ackerman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisAckerman/newest.git"
  },
  "bugs": {
    "url": "https://github.com/ChrisAckerman/newest/issues"
  },
  "homepage": "https://github.com/ChrisAckerman/newest#readme",
  "bin": "lib/index.js",
  "files": [
    "css/",
    "dist/",
    "lib/",
    "scss/"
  ],
  "keywords": [
    "update",
    "package",
    "version",
    "latest",
    "upgrade",
    "evergreen",
    "dependency",
    "dependencies"
  ],
  "scripts": {
    "test": "tsc --noEmit && tslint \"src/**/*.ts?(x)\" && cross-env NODE_ENV=test jest --passWithNoTests",
    "start": "lib/index.js",
    "clean": "del-cli lib dist css coverage && jest --clearCache",
    "prepack": "yarn run build",
    "build": "yarn run build:js && yarn run build:d.ts",
    "build:d.ts": "tsc --emitDeclarationOnly",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --extensions .ts,.tsx,.js,.jsx --copy-files"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "package-json": "^6.5.0",
    "prompts": "^2.3.2",
    "select-shell": "^1.1.3",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@types/chalk": "^2.2.0",
    "@types/compare-versions": "^3.3.0",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/prompts": "^2.4.0",
    "@types/semver": "^7.3.1",
    "@types/source-map-support": "^0.5.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.2.2",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "jest": "^26.2.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
