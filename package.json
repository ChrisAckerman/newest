{
  "name": "@xornot/tipsy",
  "description": "Package updater",
  "version": "1.0.0",
  "bin": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xornot-io/tipsy.git"
  },
  "author": "Chris Ackerman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xornot-io/tipsy/issues"
  },
  "homepage": "https://github.com/xornot-io/tipsy#readme",
  "scripts": {
    "test": "tsc --noEmit && tslint \"src/**/*.ts?(x)\" && cross-env NODE_ENV=test jest --passWithNoTests",
    "clean": "rimraf lib dist css coverage && jest --clearCache",
    "prepare": "npm run build:library",
    "build:library": "npm run build:js && npm run build:d.ts",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir lib --extensions .ts,.tsx,.js,.jsx --copy-files",
    "build:d.ts": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/compare-versions": "^3.0.0",
    "@types/prompts": "^1.2.0",
    "@types/source-map-support": "^0.5.0",
    "@xornot/memo": "^1.3.3",
    "chalk": "^2.4.2",
    "compare-versions": "^3.4.0",
    "cowsay": "^1.4.0",
    "package-json": "^6.2.0",
    "prompts": "^2.0.4",
    "source-map-support": "^0.5.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-typescript": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000"
  }
}